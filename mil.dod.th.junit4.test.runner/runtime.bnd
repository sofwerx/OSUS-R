Bundle-Name: JUnit4 XML Test Runner Bundle
Bundle-Description: \
    Junit4 XML Test Runner Bundle. Bundle should be used at runtime only, not for compiling.
Bundle-Version: 2.0.1.${tstamp}
Bundle-Vendor: UDRI
Export-Package: org.junit.*;version=4.8.2,\
    junit.*;version=4.8.2,\
    org.hamcrest.*;version=1.2,\
    mil.dod.th.ose.junit4xmltestrunner.*
Import-Package: org.apache.tools.ant;resolution:=optional,\
    org.apache.tools.ant.types;resolution:=optional,\
    org.easymock;resolution:=optional,\
    org.jmock.core;resolution:=optional,\
    org.apache.felix.service.command;status=provisional,\
    *
Private-Package: org.apache.commons.io.*;-split-package:=merge-first
Service-Component: mil.dod.th.ose.junit4xmltestrunner.*
