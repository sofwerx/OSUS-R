Bundle-Version: 1.0.4.${tstamp}
Private-Package:  \
    mil.dod.th.ose.sdk.integration
    
Test-Cases: ${classes;CONCRETE;EXTENDS;junit.framework.TestCase}

-buildpath:  \
    osgi.core;version='[5,6)',\
    osgi.cmpn,\
    biz.aQute.bnd.annotation,\
    biz.aQute.bndlib,\
    com.google.guava,\
    ${junit.integration},\
    mil.dod.th.core.api;version=latest,\
    mil.dod.th.ose.test.integration;version=latest,\
    mil.dod.th.ose.integration.commons;version=latest
-runbundles:  \
    ${osusr.core.bundles},\
    ${osusr.core.integration.bundles}
-runproperties: \
    mil.dod.th.ose.data.dir=${data.dir}, \
    target.dir=${target}, \
    sdk.dir=${workspace}/target/generic/sdk-app/deploy
