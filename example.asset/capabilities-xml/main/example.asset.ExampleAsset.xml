<AssetCapabilities xmlns="capability.asset.core.th.dod.mil"
    productName="ExampleAsset" description="An example asset."
    xmlns:p="capability.core.th.dod.mil"
    manufacturer="ExampleManufacturer">
    
    <p:primaryImage encoding="image/png"></p:primaryImage>
    
    <modalities value="Magnetic"/>
    <modalities value="Acoustic"/>
    <nominalFov>100.0</nominalFov>
    <supportedCameras id="1" type="Visible"/>
    
    <statusCapabilities sensorRangeAvailable="true" temperatureAvailable="true">
        <availableComponentStatuses type="CPU" />
        <availableComponentStatuses type="Other" description="blah" />
    </statusCapabilities> 
    
    <detectionCapabilities directionOfTravel="false" targetFrequency="true" targetId="true" targetLOB="true"
        targetLocation="true" targetOrientation="true" targetRange="false" targetSpeed="false" trackHistory="true" >
        <typesAvailable>Alarm</typesAvailable> 
        <typesAvailable>Tamper</typesAvailable>
        <typesAvailable>Test</typesAvailable>
    </detectionCapabilities>
    <digitalMediaCapabilities>
        <encodings>video/h.264</encodings>
    </digitalMediaCapabilities>
    <audioCapabilities>
        <recorders description="none" value="Microphone"/>
        <sampleRatesKHz>14.4</sampleRatesKHz>
    </audioCapabilities>   
    <videoCapabilities colorAvailable="false">
        <minResolution height="0" width="0"/>
        <maxResolution height="0" width="0"/>
        <availableCameraIDs>1</availableCameraIDs>
        <framesPerSecond>30.0</framesPerSecond>
    </videoCapabilities>
    <commandCapabilities captureData="true" performBIT="true" captureDataBySensor="false">
        <supportedCommands>GetPositionCommand</supportedCommands>
        <supportedCommands>SetPositionCommand</supportedCommands>
        <panTilt azimuthSupported="true" elevationSupported="false" bankSupported="false">
            <minAzimuth>-50.0</minAzimuth>
            <maxAzimuth>50.0</maxAzimuth>
        </panTilt>
    </commandCapabilities>
</AssetCapabilities>
